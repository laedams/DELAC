\f0\fs26 \cf2 \cb3 DefineConstant[\
length = \{10, Name "Channel length"\}\
front_distance = \{3, Name "Cylinder center distance to inlet"\}\
bottom_distance = \{2.5, Name "Cylinder center distance from bottom"\}\
width = \{5, Name "Channel width"\}\
square_length = \{1, Name "Square side length"\}\
cylinder_size = \{0.2, Name "Mesh size on cylinder"\}\
box_size = \{0.5, Name "Mesh size on wall"\}\
outer_length = \{30, Name "Domain length"\}\
coarse_size = \{0.75, Name "Mesh size on outer domain"\}\
beta = \{0, Name "Angle"\}\
flap = \{1, Name "Flap length"\}\
];\
\
// Seed the cylinder\
p = newp;\
Point(p) = \{-square_length/2, -square_length/2, 0, cylinder_size\};\
Point(p+1) = \{+square_length/2-0.01, -square_length/2, 0, cylinder_size\};\
Point(p+2) = \{+square_length/2 + flap*Cos(beta) -0.01, -square_length/2 - flap*Sin(beta), 0, cylinder_size\};\
Point(p+3) = \{+square_length/2 + flap*Cos(beta) , -square_length/2 - flap*Sin(beta) +0.01, 0, cylinder_size\};\
Point(p+4) = \{+square_length/2, -square_length/2+0.01, 0, cylinder_size\};\
\
Point(p+5) = \{+square_length/2, +square_length/2 - 0.01, 0, cylinder_size\};\
\
Point(p+6) = \{+square_length/2 + flap*Cos(beta) , +square_length/2 + flap*Sin(beta) - 0.01, 0, cylinder_size\};\
Point(p+7) = \{+square_length/2 + flap*Cos(beta) -0.01 , +square_length/2 + flap*Sin(beta) , 0, cylinder_size\};\
\
Point(p+8) = \{+square_length/2 -0.01, +square_length/2, 0, cylinder_size\};\
Point(p+9) = \{-square_length/2, +square_length/2, 0, cylinder_size\};\
\
l = newl;\
Line(l) = \{p, p+1\};\
Line(l+1) = \{p+1, p+2\};\
Line(l+2) = \{p+2, p+3\};\
Line(l+3) = \{p+3, p+4\};\
Line(l+4) = \{p+4, p+5\};\
Line(l+5) = \{p+5, p+6\};\
Line(l+6) = \{p+6, p+7\};\
Line(l+7) = \{p+7, p+8\};\
Line(l+8) = \{p+8, p+9\};\
Line(l+9) = \{p+9, p\};\
\
cylinder[] = \{l, l+1, l+2, l+3, l+4, l+5, l+6, l+7, l+8, l+9\};\
// Physical Line(1) = \{cylinder[]\};\
\
// The chanel\
p = newp;\
Point(p) = \{-front_distance, -bottom_distance, 0, box_size\};\
Point(p+1) = \{-front_distance+length, -bottom_distance, 0, box_size\};\
Point(p+2) = \{-front_distance+length, -bottom_distance+width, 0, box_size\};\
Point(p+3) = \{-front_distance, -bottom_distance+width, 0, box_size\};\
\
l = newl;\
// Bottom line\
Line(l) = \{p, p+1\};\
Line(l+1) = \{p+1, p+2\};\
Line(l+2) = \{p+2, p+3\};\
Line(l+3) = \{p+3, p\};\
\
