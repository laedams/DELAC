// Physical Line(4) = \{l+3\};\
\
// Box domain\
cframe[] = \{l, l+1, l+2, l+3\};\
\
// The surface to be mesh;\
outer = newll;\
Line Loop(outer) = \{cframe[]\};\
\
inner = newll;\
Line Loop(inner) = \{cylinder[]\};\
\
s = news;\
